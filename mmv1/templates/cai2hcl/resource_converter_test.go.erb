<%=lines(autogen_notice(:go, pwd))-%>

package <%= product_ns.downcase -%>

import (
    "fmt"
	"testing"

	cai2hcl_testing "github.com/GoogleCloudPlatform/terraform-google-conversion/v2/cai2hcl/testing"
)

<%
    resource_name = product_ns + object.name
    converter_file_name = "#{product_ns.downcase}_#{object.name.underscore}"
%>

func Test<%=resource_name-%>(t *testing.T) {
    testFileName := "<%=converter_file_name-%>"

	cai2hcl_testing.AssertTestFiles(
		t,
		ConverterNamesPerAssetType, AssetNameRegexpConverterPairs, ConverterMap,
		"./testdata",
		[]string { testFileName })
}
