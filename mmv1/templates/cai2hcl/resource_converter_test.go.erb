<%=lines(autogen_notice(:go, pwd))-%>

package <%= product_ns.downcase -%>

import (
    "fmt"
	"testing"

	cai2hcl_testing "github.com/GoogleCloudPlatform/terraform-google-conversion/v2/cai2hcl/testing"
)

<%
    resource_name = product_ns + object.name
    asset_type = product_ns + object.name + 'AssetType'
%>

func Test<%=resource_name-%>(t *testing.T) {
    assetType := <%=asset_type-%>

    testFiles, testFilesFound := ConverterNames[assetType]
	if !testFilesFound {
		panic(fmt.Sprintf("Tests not found for '%s'", assetType))
	}

	cai2hcl_testing.AssertTestFiles(
		t,
		ConverterNames, ConverterMap,
		TestsFolder,
		TestsMap[testFiles])
}
